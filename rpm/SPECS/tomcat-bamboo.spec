%define bamboo_home %{_var}/lib/bamboo/home
%define bamboo_tomcat %{_var}/lib/bamboo/tomcat
%define confdir %{bamboo_tomcat}/conf
%define workdir %{bamboo_tomcat}/work
%define tmpdir %{bamboo_tomcat}/tmp
%define logdir %{bamboo_tomcat}/log

%define tomcat_version 6.0.33
%define tomcat_home %{_usr}/share/tomcat-%{tomcat_version}
%define tomcat_user bamboo

Name: tomcat-bamboo
Version: 3.2.2

Summary: Atlassian Bamboo Tomcat Profile
Release: 1
Source0: apache-tomcat-%{tomcat_version}.tar.gz
Source1: atlassian-bamboo-3.2.2.war
Source2: tomcat-%{tomcat_version}.init
Source3: tomcat-%{tomcat_version}.sysconfig

Url: http://www.atlassian.com/software/bamboo/
Group: System Environment/Daemons

License: Proprietary

BuildArch: noarch

%description
Bamboo is a turnkey application that automates your build, test and deployment processes.

Authors:
--------
    Mark Wolfe <mark@wolfe.id.au>

%prep
%setup -q -n apache-tomcat-%{tomcat_version}

%build
# remove all the files already in base.
for i in bin lib LICENSE NOTICE RELEASE-NOTES RUNNING.txt
do
  %{__rm} -R $i
done

# don't need anything out of webapps
%{__rm} -r webapps

%install
%{__rm} -rf $RPM_BUILD_ROOT
%{__mkdir} -p ${RPM_BUILD_ROOT}%{_usr}/share/atlassian-bamboo-%{version}
%{__mkdir} -p ${RPM_BUILD_ROOT}%{bamboo_tomcat}
%{__mkdir} -p ${RPM_BUILD_ROOT}%{bamboo_home}
%{__cp} -v -p -R conf ${RPM_BUILD_ROOT}%{confdir}
%{__install} -d -m 0755 ${RPM_BUILD_ROOT}%{confdir}/Catalina/localhost
%{__install} -d -m 0775 ${RPM_BUILD_ROOT}%{logdir}
/bin/touch ${RPM_BUILD_ROOT}%{logdir}/catalina.out
%{__install} -d -m 0775 ${RPM_BUILD_ROOT}%{workdir}
%{__install} -d -m 0775 ${RPM_BUILD_ROOT}%{tmpdir}
/usr/bin/unzip "%{SOURCE1}" -d %{buildroot}%{_usr}/share/atlassian-bamboo-%{version}
%{__install} -d -m 0755 ${RPM_BUILD_ROOT}%{_initrddir}
%{__install} -d -m 0755 ${RPM_BUILD_ROOT}%{_sysconfdir}/logrotate.d
%{__install} -d -m 0755 ${RPM_BUILD_ROOT}%{_sysconfdir}/sysconfig
%{__install} -m 0644 %{SOURCE2} \
  ${RPM_BUILD_ROOT}%{_initrddir}/%{name}
%{__sed} -e "s|\@\@\@TCHOME\@\@\@|%{tomcat_home}|g" \
  -e "s|\@\@\@TCTEMP\@\@\@|%{tempdir}|g" \
  -e "s|\@\@\@TCUSER\@\@\@|%{tomcat_user}|g" \
  -e "s|\@\@\@LIBDIR\@\@\@|%{_libdir}|g" %{SOURCE3} \
  > ${RPM_BUILD_ROOT}%{_sysconfdir}/sysconfig/%{name}

%clean
%{__rm} -rf %{buildroot}

%pre
%{_sbindir}/groupadd -r %{tomcat_user} &>/dev/null || :
%{_sbindir}/useradd -g %{tomcat_user} -s /bin/false -r -c "Bamboo Continuous Build server" -d "%{buildroot}%{bamboo_home}" %{tomcat_user} &>/dev/null || :

%post

/sbin/chkconfig --add %{name}-tomcat

%files
%defattr(-,root,root,-)
%attr(0755,root,root) %{_initrddir}/%{name}
%config(noreplace) %{_sysconfdir}/sysconfig/%{name}
%attr(0775,root,tomcat) %dir %{confdir}
%attr(0775,root, tomcat) %{confdir}/Catalina
# %dir %{confdir}/Catalina
#%attr(0775,root,tomcat) %dir %{confdir}/Catalina/localhost
%config(noreplace) %{confdir}/*.policy
%config(noreplace) %{confdir}/*.properties
%config(noreplace) %{confdir}/context.xml
%config(noreplace) %{confdir}/server.xml
%attr(0664,root,tomcat) %config(noreplace) %{confdir}/tomcat-users.xml
%config(noreplace) %{confdir}/web.xml
%attr(0775,root,tomcat) %dir %{logdir}
%attr(0644,tomcat,tomcat) %{logdir}/catalina.out
%attr(0775,root,tomcat) %dir %{tmpdir}
%attr(0775,root,tomcat) %dir %{workdir}
%attr(0775,root,tomcat) %dir %{bamboo_tomcat}
%defattr(0644,root,tomcat,0755)
